{
  "name": "gatsby-remark-embed-itunes",
  "version": "1.1.0",
  "description": "Embeding iTunes tracks/albums into your Gatsby remark",
  "main": "build/main.js",
  "repository": "https://github.com/rayriffy/gatsby-remark-embed-apple-music",
